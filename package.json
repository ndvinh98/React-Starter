{
  "name": "sten-fe",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm run clear:cache && vite  --open",
    "run:dev": "npm run clear:cache && vite --mode development --open",
    "run:pro": "npm run clear:cache && vite --mode production --open",
    "build": "npm run clear:build && vite build",
    "preview": "npm run build && vite preview",
    "clear:build": "npx rimraf build",
    "clear:cache": "npx rimraf node_modules/.vite",
    "lint": "eslint src/**/*.{ts,tsx} --quiet --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix",
    "*": "prettier -w -u"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.15",
    "@chakra-ui/react": "^1.6.7",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@fontsource/lato": "^4.5.0",
    "@hookform/resolvers": "^2.8.0",
    "@rollup/plugin-image": "^2.0.6",
    "@types/node": "^16.6.2",
    "@types/react-select": "^4.0.17",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "date-fns": "^2.22.1",
    "framer-motion": "^4",
    "husky": "^6.0.0",
    "immer": "^9.0.5",
    "lint-staged": "^11.0.0",
    "lodash": "^4.17.21",
    "navi": "^0.14.0",
    "prettier": "^2.3.2",
    "pusher-js": "^7.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.0",
    "react-file-drop": "^3.1.2",
    "react-google-recaptcha": "^2.1.0",
    "react-highlight-words": "^0.17.0",
    "react-hook-form": "^7.13.0",
    "react-icons": "^4.2.0",
    "react-intl": "^5.18.1",
    "react-navi": "^0.14.4",
    "react-pro-sidebar": "^0.6.0",
    "react-select": "^4.3.1",
    "react-table": "^7.7.0",
    "rollup-plugin-uglify": "^6.0.4",
    "typescript": "^4.1.2",
    "vite": "^2.5.3",
    "vite-aliases": "^0.6.3",
    "yup": "^0.32.9",
    "zustand": "^3.5.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/lodash": "^4.14.169",
    "@types/react": "^17.0.0",
